helper def : removeRepeatedElementsInSequence(param1 : Sequence(String), param2 : Sequence(String), index : Integer) : Sequence(String) = 
	param2->iterate(p; y : Sequence(String) = Sequence{} |
		if y.indexOf(p) > 0 and thisModule.hasAlwaysSameFather(param1, p, index) then
			y
		else
			y->append(p)
		endif
	);

