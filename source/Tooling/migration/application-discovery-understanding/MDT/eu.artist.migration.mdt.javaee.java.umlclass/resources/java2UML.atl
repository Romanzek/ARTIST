--	param2: internal
helper def : hasAlwaysSameFather(param1 : Sequence(String), param2 : String, index : Integer) : Boolean =
	thisModule.elementsAreEqual(
		param1->select(e|e.indexOf(param2) = index) -- > 0?
		->iterate(p; y : Sequence(String) = Sequence{} |
			if p.indexOf(param2) > 1 then --not the first element (ex. not 'net')
				y->append(p.at(p.indexOf(param2) - 1))
			else
				y->append(p.at(1))
			endif
			
		)
	)
;

