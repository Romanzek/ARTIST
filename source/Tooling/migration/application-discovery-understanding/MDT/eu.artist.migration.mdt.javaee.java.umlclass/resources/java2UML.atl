helper def : insertNewSequence (list : Sequence(OclAny), father : String, node : String) : Sequence(OclAny) =
	--if list only contains strings
	if not thisModule.areSequencesInside(list) then
		--if father is in the list, append node
		if (list.indexOf(father) > 0) and (list->last() = father) then
			list->append(Sequence{node})
		else
			list
		endif
	--list contains sequences
	else
		--if any of the elemts (type String) is the father, insert node
		if list.indexOf(father) > 0 then
			list->append(Sequence{node})
		--iterate, calling insertInSequenceContainingElement on sequences
		else
			list->iterate(p; y : Sequence(OclAny) = Sequence{} |
			if p.oclIsTypeOf(Sequence(OclAny)) then
				y->append(thisModule.insertNewSequence(p, father, node))
			else
				y->append(p)
			endif
			)
		endif
	endif
;

--***************Unresolved items***************
rule JUnresolvedItemToUmlClass {
	from 
		jUnresItem : JAVA!UnresolvedItem(
			--all the UnresolvedItems are transformed the first time
			jUnresItem.isFirstUnresolvedItem and --MODIF
			jUnresItem.usagesInImports -> notEmpty()	
			and 
			(
				jUnresItem.getCorrespondingInterfDec.oclIsTypeOf(Sequence(JAVA!ClassDeclaration))					
				or
				jUnresItem.getCorrespondingEnumDec.oclIsTypeOf(Sequence(JAVA!ImportDeclaration))
			)
		)
	to 
		--MODIF
--		umlNamedElement : UML!Class(
--			name <- jUnresItem.name,
--			package <- thisModule.createExternalsModel(thisModule.jModelRoot)
--		)
		umlNamedElement : UML!Package(
			name <- jUnresItem.name,
			nestingPackage <- thisModule.createExternalsModel(thisModule.jModelRoot)
		)
	do{
		
		
		'initialPackageHierarchy: '.concat(thisModule.initialPackageHierarchy.toString()).println();
		
		'TEST insertInSequenceContainingElement refractions: '.concat(thisModule.insertInSequenceContainingElement(Sequence{Sequence{'net'}, Sequence{'org'}}, 'net', 'refractions').toString()).println();
		'TEST insertNewSequence dews_online: '.concat(thisModule.insertNewSequence(Sequence{Sequence{'net'}, Sequence{'org'}}, 'org', 'dews_online').toString()).println();
		--packageHierarchy es un fragmento de initialPackageHierarchy
		thisModule.packageHierarchy <- Sequence {Sequence {'net', 'refractions', 'udig', 'project', 'ui', 'internal', 'MapEditor'}, Sequence {'net', 'refractions', 'udig', 'tool', 'info', 'internal', 'InfoView2'}, Sequence {'org', 'dews_online', 'ccui', 'core', 'Activator'}, Sequence {'org', 'eclipse', 'jface', 'dialogs', 'IDialogSettings'}, Sequence {'org', 'eclipse', 'ui', 'IFolderLayout'}, Sequence {'org', 'eclipse', 'ui', 'IPageLayout'}, Sequence {'org', 'eclipse', 'ui', 'IPerspectiveFactory'}};				
		for (iterator in thisModule.longestPath(thisModule.packageHierarchy)){ --0-7
			--currentPathSections: {{net, org}, {refractions, dews_online, eclipse}, ...}
			thisModule.currentPathSections <- thisModule.pathSections(thisModule.packageHierarchy, thisModule.index);
			'index: '.concat(thisModule.index.toString()).println();
			if(thisModule.index = 1){ --initialize
					thisModule.packageHierarchyTemp <- Sequence{};
			}
			thisModule.equalCounter <- thisModule.numberOfRepeatedElements(thisModule.pathSections(thisModule.packageHierarchy, thisModule.index));
			if(thisModule.equalCounter = 0){
				thisModule.equalCounter <- 1;
			}
			'pathSections: '.concat(thisModule.pathSections(thisModule.packageHierarchy, thisModule.index)).println();
			--'equalCounter: '.concat(thisModule.equalCounter.toString()).println();
			--'repeatedElement: '.concat(thisModule.repeatedElement(thisModule.pathSections(thisModule.packageHierarchy, thisModule.index))).println();
			for(iterator2 in thisModule.pathSections(thisModule.packageHierarchy, thisModule.index)){ --net, org, ...
				thisModule.currentNode <- thisModule.pathSections(thisModule.packageHierarchy, thisModule.index).at(thisModule.index2);
				'currentNode: '.concat(thisModule.currentNode).println();
				if(thisModule.index = 1){ --fill with first level nodes
					thisModule.packageHierarchyTemp <- thisModule.packageHierarchyTemp
					->append(Sequence{thisModule.currentNode});
				}else{
					'father: '.concat(thisModule.getFather(thisModule.packageHierarchy, thisModule.currentNode, thisModule.index, thisModule.equalCounter)).println();
					'isUniqueSon: '.concat(thisModule.isUniqueSon(thisModule.packageHierarchy, thisModule.currentNode, thisModule.index, thisModule.equalCounter).toString()).println();
						
					--'isUniqueSon param1: '.concat(thisModule.packageHierarchy->select(e|e.indexOf(thisModule.getFather(thisModule.packageHierarchy, thisModule.currentNode, thisModule.index, thisModule.equalCounter)) > 0).size().toString()).println();
					--'isUniqueSon param2: '.concat(thisModule.packageHierarchy->select(e|e.indexOf(thisModule.currentNode) = thisModule.index).size().toString()).println();--cuyo padre sea getFather...
					--Insert the node in packageHierarchyTemp
					--this if is use for the case when there are several nodes in the same level with different father. Ex. internal
					if(thisModule.currentNode = thisModule.repeatedElement(thisModule.pathSections(thisModule.packageHierarchy, thisModule.index))){
						
						--1)In the father's sequence: when the father does not have different children. Example: refractions
						if(thisModule.isUniqueSon(thisModule.packageHierarchy, thisModule.currentNode, thisModule.index, thisModule.equalCounter)){
							thisModule.packageHierarchyTemp <- 
							thisModule.insertInSequenceContainingElement(thisModule.packageHierarchyTemp, thisModule.getFather(thisModule.packageHierarchy, thisModule.currentNode, thisModule.index, thisModule.equalCounter), thisModule.currentNode);
							'packageHierarchyTemp: '.concat(thisModule.packageHierarchyTemp.toString()).println();
						--2)In a new sequence: when the father have different children. Example: dews_online
						}else{
							thisModule.packageHierarchyTemp <-
							thisModule.insertNewSequence(thisModule.packageHierarchyTemp, thisModule.getFather(thisModule.packageHierarchy, thisModule.currentNode, thisModule.index, thisModule.equalCounter), thisModule.currentNode);
							'packageHierarchyTemp: '.concat(thisModule.packageHierarchyTemp.toString()).println();
						}
						thisModule.equalCounter <- thisModule.equalCounter-1;
					}else{
						--1)In the father's sequence: when the father does not have different children. Example: refractions
						if(thisModule.isUniqueSon(thisModule.packageHierarchy, thisModule.currentNode, thisModule.index, 1)){
							thisModule.packageHierarchyTemp <- 
							thisModule.insertInSequenceContainingElement(thisModule.packageHierarchyTemp, thisModule.getFather(thisModule.packageHierarchy, thisModule.currentNode, thisModule.index, 1), thisModule.currentNode);
							'packageHierarchyTemp: '.concat(thisModule.packageHierarchyTemp.toString()).println();
						--2)In a new sequence: when the father have different children. Example: dews_online
						}else{
							thisModule.packageHierarchyTemp <-
							thisModule.insertNewSequence(thisModule.packageHierarchyTemp, thisModule.getFather(thisModule.packageHierarchy, thisModule.currentNode, thisModule.index, 1), thisModule.currentNode);
							'packageHierarchyTemp: '.concat(thisModule.packageHierarchyTemp.toString()).println();
						}
					}
				}
				thisModule.index2 <- thisModule.index2 + 1;
			}
			thisModule.index <- thisModule.index + 1;
			thisModule.index2 <- 1;
			
		}
		'packageHierarchy final: '.concat(thisModule.packageHierarchyTemp.toString()).println();
		
--		if(jUnresItem.name.indexOf('.') > 0){
--			umlNamedElement.nestedPackage <- thisModule.createPackage(jUnresItem);
--		}else{
--			umlNamedElement.packagedElement <- thisModule.createClass(jUnresItem);
--		}
	}
}

