helper def : insertInside(param1 : Sequence(String), param2 : OclAny) : Sequence(String) = 
	param1->iterate(p; y : Sequence(String) = Sequence{} |
		if param1->first() = p then
			y->append(p->append(param2))
		else
			y->append(p)
		endif
	);
--returns if param2 is in param1 (recursive)
helper def : hasInside (param1 : Sequence(String), param2 : String) : Boolean =
	if param1.oclIsTypeOf(String) then
		if param1 = param2 then
			true
		else
			false
		endif
	else
		param1->iterate(p; y : Boolean = false |
		if p.oclIsTypeOf(String) then 
			if p = param2 then
				true
			else
				y
			endif
		else --if sequence, call again
			if y = true then
				true
			else
				thisModule.hasInside(p, param2)
			endif
		endif
		)
	endif
	;
